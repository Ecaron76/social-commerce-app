generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgres"
  url = env("DATABASE_URL")
}

model Category {
  id          Int    @id @default(autoincrement())
  name        String @db.VarChar(100)
  slug        String @db.VarChar(100)
  description String @db.VarChar(300)
}
